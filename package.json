{
    "name": "zolatik",
    "version": "1.0.2",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open --configuration development",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "prod-build": "ng build --configuration production --output-path ./dist/prod/prod-dist --optimization=true",
        "beta-build": "npm run release:beta && ng build --configuration beta --output-path ./dist/beta/beta-dist --optimization=true",
        "dev-build": "ng build --configuration development --output-path ./dist/dev/dev-dist --optimization=true",
        "test-build": "ng build --configuration test --output-path ./dist/test/test-dist --optimization=true",
        "prettier": "prettier --write src/**/*.{html,ts,css,scss}",
        "pretty-quick": "pretty-quick",
        "release:major": "node scripts/bump-version.js major",
        "release:minor": "node scripts/bump-version.js minor",
        "release:patch": "node scripts/bump-version.js patch",
        "release:beta": "node scripts/bump-version.js patch beta",
        "release:dev": "node scripts/bump-version.js patch",
        "release:prod": "node scripts/bump-version.js patch",
        "eslint": "eslint --color --fix -c .eslintrc.json --ext .ts ./src/app"
    },
    "private": true,
    "dependencies": {
