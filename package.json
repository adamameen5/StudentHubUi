        "@matheo/text-mask": "^17.0.0",
        "@ng-select/ng-select": "^13.7.0",
        "@ngrx/store": "^18.0.2",
        "@ngx-translate/core": "^15.0.0",
        "@ngx-translate/http-loader": "^8.0.0",
